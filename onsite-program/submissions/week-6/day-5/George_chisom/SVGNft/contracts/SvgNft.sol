    function mint(address to, uint256 id, uint256 amount, bytes calldata data) external override onlyOwner {
        if (id != token_id) {
            revert SvgStorage.Invalid_token_id();
        } else if (balanceOf(address(this), id) + amount >= full_supply) {
            revert SvgStorage.Exceeds_Token_Supply();
        } else {
            _mint(to, id, amount, data);
        }
    }

    function generate_svg_with_time(uint _token_id) external view returns (string memory) {
        if (_token_id != token_id) {
            revert SvgStorage.Invalid_token_id();
        }

        string memory my_timestamp = Strings.toString(block.timestamp);

        string memory svg = string(abi.encodePacked(
            svgParts[0], // SVG header
            svgParts[1], // Path start
            svgParts[2], // Path continuation
            svgParts[3],
            '<text x="20" y="150" font-family="Arial" font-size="20" fill="black">',
            'Time: ',
            my_timestamp,
            '</text>',
            svgParts[4]
            )
        );

        return svg;
    }

    function token_uri(uint256 _token_id) public view override returns (string memory) {

        if (_token_id != token_id) {
            revert SvgStorage.Invalid_token_id();
        }

        string memory svg = this.generate_svg_with_time(_token_id);

        string memory svgBase64 = Base64.encode(bytes(svg));

        string memory json = string(
            abi.encodePacked(
                '{',
                '"name": "Leo Nft', Strings.toString(token_id), '",',
                '"description": "An NFT displaying the current blockchain timestamp",',
                '"image": "data:image/svg+xml;base64,', svgBase64, '",',
                '"attributes": [{"trait_type": "Timestamp", "value": "', Strings.toString(block.timestamp), '"}]',
                '}'
            )
        );

        
        string memory jsonBase64 = Base64.encode(bytes(json));

    
        return string(abi.encodePacked("data:application/json;base64,", jsonBase64));
    }

    
